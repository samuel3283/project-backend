@Library('jenkins_shared_library@master')
import pe.com.sura.services.JenkinsCloudFormation
def pipeline = new JenkinsCloudFormation(this)

def project="APLICATION004"
def environment="DEV"

node {

   stage('Preparation') {
      checkout scm
      pipeline.configure()
      env.project="${project}"
   }
   stage('Deploy') {
      pipeline.deployS3IaC()
   }

}

