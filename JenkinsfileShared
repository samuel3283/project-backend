@Library('jenkins_shared_library@master')
def pipeline = new pe.com.sura.services.JenkinsCloudFormation(this)

def project="APLICATION001"
def environment="DEV"

node {

   stage('Preparation') {
      checkout scm
      pipeline.configure()
      env.project="${project}"
   }
   stage('Deploy') {
      pipeline.deployS3IaC()
   }

}

